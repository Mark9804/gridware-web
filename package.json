{
  "name": "gridware-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --config .prettierrc.json -uw ./src/ *.md *.ts",
    "build": "pnpm format; vue-tsc && vite build",
    "build:github": "pnpm build && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build; vite preview --host",
    "check": "eslint --config ./.eslintrc.json ./src/ --ext .js,.ts,.vue"
  },
  "dependencies": {
    "lodash-es": "4.17.21",
    "pinia": "2.0.26",
    "pinia-plugin-persistedstate": "2.4.0",
    "vue": "3.2.45",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@trivago/prettier-plugin-sort-imports": "3.4.0",
    "@types/lodash-es": "4.17.6",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "@vitejs/plugin-legacy": "2.3.1",
    "@vitejs/plugin-vue": "3.2.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "chalk": "5.1.2",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.8.0",
    "postcss": "8.4.19",
    "prettier": "2.8.0",
    "prettier-plugin-css-order": "1.3.0",
    "rollup-plugin-visualizer": "5.8.3",
    "sass": "1.56.1",
    "terser": "5.16.0",
    "typescript": "4.9.3",
    "vite": "3.2.4",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.13.3",
    "vue-tsc": "1.0.9"
  }
}
