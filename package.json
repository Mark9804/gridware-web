{
  "name": "gridware-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --config .prettierrc.json -uw ./src/ *.md *.ts",
    "build": "pnpm format; vite build",
    "build:github": "pnpm build && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build; vite preview --host",
    "check": "eslint --config ./.eslintrc.json ./src/ --ext .js,.ts,.vue",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue}": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ],
    "*.md": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ]
  },
  "dependencies": {
    "@antv/g2plot": "2.4.31",
    "@antv/g6": "4.8.24",
    "@types/node": "20.11.5",
    "@vue/compiler-sfc": "3.x",
    "husky": "8.0.3",
    "lodash-es": "4.17.21",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "3.2.1",
    "uuid": "9.0.1",
    "vue": "3.4.14",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.7.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/lodash-es": "4.17.12",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@vitejs/plugin-legacy": "5.2.0",
    "@vitejs/plugin-vue": "4.6.2",
    "@vue/eslint-config-typescript": "12.0.0",
    "autoprefixer": "10.4.17",
    "chalk": "5.3.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.20.1",
    "naive-ui": "2.37.3",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "prettier-plugin-css-order": "2.0.1",
    "rollup-plugin-visualizer": "5.12.0",
    "sass": "1.69.7",
    "tailwindcss": "3.4.1",
    "terser": "5.27.0",
    "typescript": "5.3.3",
    "unplugin-auto-import": "0.17.3",
    "unplugin-vue-components": "0.26.0",
    "vite": "5.0.11",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.17.4",
    "vue-tsc": "1.8.27"
  }
}
