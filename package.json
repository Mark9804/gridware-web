{
  "name": "gridware-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --config .prettierrc.json -uw ./src/ *.md *.ts",
    "build": "pnpm format; vite build",
    "build:github": "pnpm build && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build; vite preview --host",
    "check": "eslint --config ./.eslintrc.json ./src/ --ext .js,.ts,.vue",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue}": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ],
    "*.md": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ]
  },
  "dependencies": {
    "@antv/g6": "4.8.15",
    "@types/node": "20.3.0",
    "@vue/compiler-sfc": "3.x",
    "husky": "^8.0.3",
    "lodash-es": "4.17.21",
    "p5": "1.6.0",
    "pinia": "2.1.3",
    "pinia-plugin-persistedstate": "3.1.0",
    "uuid": "9.0.0",
    "vue": "3.3.4",
    "vue-router": "4.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.3.1",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/lodash-es": "4.17.7",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "@vitejs/plugin-legacy": "4.0.4",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/eslint-config-typescript": "11.0.2",
    "autoprefixer": "10.4.14",
    "chalk": "5.2.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.14.1",
    "naive-ui": "2.34.4",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-css-order": "1.3.1",
    "rollup-plugin-visualizer": "5.9.0",
    "sass": "1.63.2",
    "tailwindcss": "3.3.2",
    "terser": "5.17.7",
    "typescript": "5.1.3",
    "unplugin-auto-import": "0.16.4",
    "unplugin-vue-components": "0.25.1",
    "vite": "4.3.9",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.16.3",
    "vue-tsc": "1.6.5"
  }
}
