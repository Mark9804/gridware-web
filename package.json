{
  "name": "gridware-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --config .prettierrc.json -uw ./src/ *.md *.ts",
    "build": "pnpm format; vite build",
    "build:github": "pnpm build && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build; vite preview --host",
    "check": "eslint --config ./.eslintrc.json ./src/ --ext .js,.ts,.vue",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue}": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ],
    "*.md": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ]
  },
  "dependencies": {
    "@antv/g2plot": "2.4.31",
    "@antv/g6": "4.8.24",
    "@types/node": "20.14.12",
    "@vue/compiler-sfc": "3.x",
    "husky": "9.1.2",
    "lodash-es": "4.17.21",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "3.2.1",
    "uuid": "9.0.1",
    "vite-plugin-vue-devtools": "^7.3.6",
    "vue": "3.4.34",
    "vue-router": "4.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.3",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/lodash-es": "4.17.12",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "@vitejs/plugin-legacy": "5.4.1",
    "@vitejs/plugin-vue": "5.1.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "autoprefixer": "10.4.19",
    "chalk": "5.3.0",
    "eslint": "9.7.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-sort-exports": "0.9.1",
    "eslint-plugin-vue": "9.27.0",
    "naive-ui": "2.39.0",
    "postcss": "8.4.40",
    "prettier": "3.3.3",
    "prettier-plugin-css-order": "2.1.2",
    "rollup-plugin-visualizer": "5.12.0",
    "sass": "1.77.8",
    "tailwindcss": "3.4.7",
    "terser": "5.31.3",
    "typescript": "5.5.4",
    "unplugin-auto-import": "0.18.2",
    "unplugin-vue-components": "0.27.3",
    "vite": "5.3.5",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.20.1",
    "vue-tsc": "2.0.29"
  }
}
