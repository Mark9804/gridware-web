{
  "name": "gridware-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --config .prettierrc.json -uw ./src/ *.md *.ts",
    "build": "pnpm format; vite build",
    "build:github": "pnpm build && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build; vite preview --host",
    "check": "eslint --config ./.eslintrc.json ./src/ --ext .js,.ts,.vue"
  },
  "dependencies": {
    "@vue/compiler-sfc": "3.x",
    "lodash-es": "4.17.21",
    "p5": "1.6.0",
    "pinia": "2.0.34",
    "pinia-plugin-persistedstate": "3.1.0",
    "uuid": "9.0.0",
    "vue": "3.2.47",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/lodash-es": "4.17.7",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "@vitejs/plugin-legacy": "4.0.2",
    "@vitejs/plugin-vue": "4.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "chalk": "5.2.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.10.0",
    "naive-ui": "2.34.3",
    "postcss": "8.4.21",
    "prettier": "2.8.7",
    "prettier-plugin-css-order": "1.3.0",
    "rollup-plugin-visualizer": "5.9.0",
    "sass": "1.62.0",
    "terser": "5.16.9",
    "typescript": "5.0.4",
    "unplugin-auto-import": "0.15.2",
    "unplugin-vue-components": "0.24.1",
    "vite": "4.2.1",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.14.7",
    "vue-tsc": "1.2.0"
  }
