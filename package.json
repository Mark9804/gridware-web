{
  "name": "gridware-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --config .prettierrc.json -uw ./src/ *.md *.ts",
    "build": "pnpm format; vite build",
    "build:github": "pnpm build && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build; vite preview --host",
    "check": "eslint --config ./.eslintrc.json ./src/ --ext .js,.ts,.vue",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue}": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ],
    "*.md": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ]
  },
  "dependencies": {
    "@antv/g2plot": "2.4.31",
    "@antv/g6": "4.8.22",
    "@types/node": "20.7.0",
    "@vue/compiler-sfc": "3.x",
    "husky": "8.0.3",
    "lodash-es": "4.17.21",
    "pinia": "2.1.6",
    "pinia-plugin-persistedstate": "3.2.0",
    "uuid": "9.0.1",
    "vue": "3.3.4",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.5.0",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/lodash-es": "4.17.9",
    "@types/uuid": "9.0.4",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "@vitejs/plugin-legacy": "4.1.1",
    "@vitejs/plugin-vue": "4.3.4",
    "@vue/eslint-config-typescript": "12.0.0",
    "autoprefixer": "10.4.16",
    "chalk": "5.3.0",
    "eslint": "8.50.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.17.0",
    "naive-ui": "2.34.4",
    "postcss": "8.4.30",
    "prettier": "3.0.3",
    "prettier-plugin-css-order": "1.3.1",
    "rollup-plugin-visualizer": "5.9.2",
    "sass": "1.68.0",
    "tailwindcss": "3.3.3",
    "terser": "5.20.0",
    "typescript": "5.2.2",
    "unplugin-auto-import": "0.16.6",
    "unplugin-vue-components": "0.25.2",
    "vite": "4.4.9",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.16.5",
    "vue-tsc": "1.8.15"
  }
}
