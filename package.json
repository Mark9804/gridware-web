{
  "name": "gridware-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --config .prettierrc.json -uw ./src/ *.md *.ts",
    "build": "pnpm format; vite build",
    "build:github": "pnpm build && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build; vite preview --host",
    "check": "eslint --config ./.eslintrc.json ./src/ --ext .js,.ts,.vue",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue}": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ],
    "*.md": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ]
  },
  "dependencies": {
    "@antv/g2plot": "2.4.32",
    "@antv/g6": "5.0.21",
    "@types/node": "20.16.5",
    "@vue/compiler-sfc": "3.x",
    "husky": "9.1.6",
    "lodash-es": "4.17.21",
    "pinia": "2.2.2",
    "pinia-plugin-persistedstate": "4.0.2",
    "uuid": "10.0.0",
    "vite-plugin-vue-devtools": "^7.3.6",
    "vue": "3.5.4",
    "vue-router": "4.4.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.4",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/lodash-es": "4.17.12",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.5.0",
    "@typescript-eslint/parser": "8.5.0",
    "@vitejs/plugin-legacy": "5.4.2",
    "@vitejs/plugin-vue": "5.1.3",
    "@vue/eslint-config-typescript": "13.0.0",
    "autoprefixer": "10.4.20",
    "chalk": "5.3.0",
    "eslint": "9.10.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-sort-exports": "0.9.1",
    "eslint-plugin-vue": "9.28.0",
    "naive-ui": "2.39.0",
    "postcss": "8.4.45",
    "prettier": "3.3.3",
    "prettier-plugin-css-order": "2.1.2",
    "rollup-plugin-visualizer": "5.12.0",
    "sass": "1.78.0",
    "tailwindcss": "3.4.11",
    "terser": "5.32.0",
    "typescript": "5.6.2",
    "unplugin-auto-import": "0.18.3",
    "unplugin-vue-components": "0.27.4",
    "vite": "5.4.4",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.20.5",
    "vue-tsc": "2.1.6"
  }
}
