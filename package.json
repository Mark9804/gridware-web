{
  "name": "gridware-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --config .prettierrc.json -uw ./src/ *.md *.ts",
    "build": "pnpm format; vite build",
    "build:github": "pnpm build && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build; vite preview --host",
    "check": "eslint --config ./.eslintrc.json ./src/ --ext .js,.ts,.vue"
  },
  "dependencies": {
    "@vue/compiler-sfc": "3.x",
    "lodash-es": "4.17.21",
    "pinia": "2.0.28",
    "pinia-plugin-persistedstate": "3.0.1",
    "uuid": "9.0.0",
    "vue": "3.2.45",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/lodash-es": "4.17.6",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "@vitejs/plugin-legacy": "3.0.1",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "chalk": "5.2.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.8.0",
    "naive-ui": "2.34.2",
    "postcss": "8.4.20",
    "prettier": "2.8.1",
    "prettier-plugin-css-order": "1.3.0",
    "rollup-plugin-visualizer": "5.8.3",
    "sass": "1.57.0",
    "typescript": "4.9.4",
    "vite": "4.0.2",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.14.0",
    "vue-tsc": "1.0.14"
  }
}
